{
  "name": "movigo-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Express server with TypeScript, Knex, and PostgreSQL for the moviGo application",
  "main": "dist/index.js",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "migrate": "tsx -r tsx/register node_modules/.bin/knex migrate:latest",
    "migrate:status": "tsx -r tsx/register node_modules/.bin/knex knex migrate:status",
    "seed": "tsx -r tsx/register node_modules/.bin/knex seed:run",
    "build:packages": "npm run build --workspaces",
    "dev:packages": "npm run dev --workspaces",
    "web:dev": "cd packages/web && pnpm dev",
    "web:build": "cd packages/web && pnpm build",
    "web:start": "cd packages/web && pnpm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run web:dev\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/s3-request-presigner": "^3.454.0",
    "@movigo/api": "workspace:*",
    "@movigo/auth": "workspace:*",
    "@movigo/database": "workspace:*",
    "@movigo/types": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "resend": "^6.0.1",
    "superjson": "^2.2.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
